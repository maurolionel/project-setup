{
  "name": "polareact",
  "version": "1.0.0",
  "description": "ReactJs",
  "scripts": {
    "clean:dist": "rimraf ./dist",
    "message": "babel-node ./build/message.js",
    "build:dev": "cross-env NODE_ENV=local babel-node build/build-dev.js",
    "build:prod": "babel-node ./build/build-prod.js",
    "prelocal": "cross-env NODE_ENV=local npm-run-all --parallel clean:dist message",
    "local": "cross-env NODE_ENV=local npm-run-all build:dev",
    "prestg": "cross-env NODE_ENV=staging npm-run-all clean:dist message",
    "stg": "cross-env NODE_ENV=staging npm-run-all --parallel testjs build:prod",
    "preprod": "cross-env NODE_ENV=production npm-run-all clean:dist message",
    "prod": "cross-env NODE_ENV=production npm-run-all --parallel build:prod",
    "testjs": "cross-env NODE_ENV=production jest",
    "start": "npm-run-all local",
    "coverage": "cross-env NODE_ENV=test nyc jest",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "author": "React",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-register": "^6.24.1",
    "body-parser": "^1.17.1",
    "colors": "^1.1.2",
    "cross-env": "^5.0.1",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^20.0.4",
    "nock": "^9.0.13",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "nyc": "^11.0.2",
    "postcss-loader": "^2.0.5",
    "react-addons-test-utils": "^15.5.1",
    "redux-mock-store": "^1.2.3",
    "regenerator-runtime": "^0.10.5",
    "rimraf": "^2.6.1",
    "string-replace-webpack-plugin": "^0.1.3",
    "sw-precache-webpack-plugin": "^0.11.3",
    "url-loader": "^0.5.8",
    "webpack": "^2.4.1",
    "webpack-config": "^7.0.0",
    "webpack-dev-server": "^2.4.4",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-manifest-plugin": "^1.1.0"
  },
  "dependencies": {
    "auto-breadcrumb": "^1.1.0",
    "babel-cli": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "downshift": "^1.6.0",
    "history": "^4.6.1",
    "lodash": "^4.17.4",
    "match-sorter": "^1.8.1",
    "polished": "^1.2.1",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-intl": "^2.3.0",
    "react-redux": "^5.0.4",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "^5.0.0-alpha.6",
    "react-slick": "^0.15.4",
    "react-virtualized": "^9.10.1",
    "redux": "^3.6.0",
    "redux-api-middleware": "^1.0.3",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "store": "^2.0.12",
    "styled-components": "^2.0.1",
    "styled-components-theme": "^1.0.5"
  }
}
